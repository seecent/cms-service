<?xml version="1.0" encoding="utf-8"?>
<!--数据导入配置模板-->
<Constant name="constants" table="lms_constants" label="枚举类型" validate="true" merge="true">
    <!--去重主键设置-->
    <UniqueKeys>
		<UniqueKey name="code" mapping="CODE"/>
		<UniqueKey name="const_type" mapping="NAME"/>
	</UniqueKeys>
	<Columns>
		<Column name="code" mapping="CODE" type="String" nullable="false" label="编码"/>
		<Column name="name" mapping="NAME" type="String" nullable="false" label="名称"/>
		<Column name="const_type" mapping="TYPE" type="String" nullable="false" label="类型名称"/>
		<Column name="const_label" mapping="LABEL" type="String" label="类型显示名称"/>
		<Column name="const_alias" mapping="ALIAS" type="String" label="别名"/>
		<Column name="order_no" mapping="ORDERNO" type="Integer" label="排序"/>
	</Columns>
</Constant>
